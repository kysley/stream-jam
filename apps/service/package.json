{
  "name": "service",
  "version": "1.0.0",
  "author": "Evan Kysley",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsup src/index.ts --format cjs --dts --watch --onSuccess \"node -r dotenv/config dist/index.js\"",
    "build:schema": "node build-schema.js && graphql-codegen",
    "db:generate": "yarn prisma generate",
    "db:push": "yarn prisma db push",
    "db:up": "docker-compose -p stream-jam up -d"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/node": "^18.15.11",
    "@types/ws": "^8.5.4",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/jwt": "^6.7.1",
    "@fastify/websocket": "^7.2.0",
    "@prisma/client": "^4.12.0",
    "@trpc/server": "^10.18.0",
    "bullmq": "^3.10.2",
    "database": "*",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-socket.io": "^4.0.0",
    "got": "^12.5.3",
    "immer": "^9.0.21",
    "ioredis": "^5.3.1",
    "mercurius": "^12.2.0",
    "prisma": "^4.12.0",
    "socket.io": "^4.6.1",
    "types": "*",
    "wretch": "^2.5.1",
    "zod": "^3.21.4"
  }
}
