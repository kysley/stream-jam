{
  "name": "service",
  "version": "1.0.0",
  "author": "Evan Kysley",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsup src/index.ts --format cjs --dts --watch --onSuccess \"node -r dotenv/config dist/index.js\"",
    "build:schema": "node build-schema.js && graphql-codegen",
    "db:generate": "yarn prisma generate",
    "db:push": "yarn prisma db push",
    "db:up": "docker-compose -p stream-jam up -d"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/node": "^18.19.3",
    "@types/ws": "^8.5.10",
    "rimraf": "^5.0.5",
    "tsup": "^7.3.0",
    "tsx": "^3.14.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@fastify/websocket": "^8.3.0",
    "@prisma/client": "^5.3.1",
    "@trpc/server": "^10.44.1",
    "@twurple/api": "^7.0.6",
    "@twurple/auth": "^7.0.6",
    "@twurple/eventsub-http": "^7.0.6",
    "@twurple/eventsub-ngrok": "^7.0.6",
    "@twurple/eventsub-ws": "^7.0.6",
    "bullmq": "^4.15.4",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "fastify-socket.io": "^5.0.0",
    "immer": "^10.0.3",
    "ioredis": "^5.3.2",
    "prisma": "^5.7.0",
    "socket.io": "^4.7.2",
    "tesjs": "^1.0.4",
    "types": "*",
    "wretch": "^2.7.1",
    "zod": "^3.22.4"
  }
}
