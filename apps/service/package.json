{
  "name": "service",
  "version": "1.0.0",
  "author": "Evan Kysley",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsup src/index.ts --format cjs --dts --watch --onSuccess \"node -r dotenv/config dist/index.js\"",
    "db:generate": "yarn prisma generate",
    "db:push": "yarn prisma db push",
    "db:up": "docker-compose -p stream-jam up -d",
    "db:studio": "yarn prisma studio"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/node": "^18.19.24",
    "@types/ws": "^8.5.10",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.11.0",
    "@trpc/server": "^10.45.2",
    "@twurple/api": "^7.0.10",
    "@twurple/auth": "^7.0.10",
    "@twurple/eventsub-http": "^7.0.10",
    "@twurple/eventsub-ngrok": "^7.0.10",
    "@twurple/eventsub-ws": "^7.0.10",
    "bullmq": "^4.17.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-socket.io": "^5.0.0",
    "immer": "^10.0.4",
    "ioredis": "^5.3.2",
    "prisma": "^5.11.0",
    "socket.io": "^4.7.5",
    "tesjs": "^1.0.4",
    "types": "*",
    "wretch": "^2.8.1",
    "zod": "^3.22.4"
  }
}
